{
  "manifest_version": 3,
  "name": "UnDiffused",
  "description": "Privacy-first AI image detection. All processing happens locally on your device.",
  "version": "1.0.0",
  "icons": {
    "16": "icons/icon16.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  },
  "permissions": [
    "contextMenus",
    "activeTab"
  ],
  "host_permissions": [
    "https://*/*",
    "http://*/*"
  ],
  "action": {
    "default_popup": "src/popup/popup.html"
  },
  "background": {
    "service_worker": "service-worker-loader.js",
    "type": "module"
  },
  "content_scripts": [
    {
      "js": [
        "assets/index.tsx-loader-BlayQZjj.js"
      ],
      "matches": [
        "https://*/*",
        "http://*/*"
      ],
      "css": []
    }
  ],
  "content_security_policy": {
    "extension_pages": "script-src 'self' 'wasm-unsafe-eval'; object-src 'self'"
  },
  "web_accessible_resources": [
    {
      "matches": [
        "http://*/*",
        "https://*/*"
      ],
      "resources": [
        "models/model_quantized.onnx",
        "wasm/ort-wasm-simd.wasm",
        "wasm/ort-wasm.wasm",
        "wasm/ort-wasm-threaded.wasm",
        "wasm/ort-wasm-simd-threaded.wasm"
      ],
      "use_dynamic_url": true
    },
    {
      "matches": [
        "http://*/*",
        "https://*/*"
      ],
      "resources": [
        "assets/GlassCard-BMBPpFOK.js",
        "assets/index.tsx-D6WKwmLH.js"
      ],
      "use_dynamic_url": false
    }
  ]
}
